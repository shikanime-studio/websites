---
import NavbarStart from "./NavbarStart.astro";
import NavbarCenter from "./NavbarCenter.astro";
import NavbarEnd from "./NavbarEnd.astro";

interface Props {
  session?: {
    user: {
      name: string;
      image?: string | null;
    };
  } | null;
}
---

<nav class="navbar fixed top-0 z-50 w-full bg-base-100 px-4 lg:px-6 justify-between">
  <NavbarStart />
  <NavbarCenter />
  <NavbarEnd session={Astro.props.session} />
</nav>
